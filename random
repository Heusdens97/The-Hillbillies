public static int[][] RandomArray(int end, int length, int arrays){
		int[][] ArrayList = new int[arrays][length];
		for (int i = 0; i < arrays; i++){
			for (int j = 0; j < length; j++){
				Random rand = new Random();
				int n = rand.nextInt(end+1); // genereert random getal tussen inclusief 0 en exclusief de parameter
				ArrayList[i][j] = n;
			}
		}
		return ArrayList;
	}
